{
  "name": "markdown-dialect-format",
  "displayName": "md-maic",
  "description": "markdown dialect format",
  "version": "0.0.6",
  "publisher": "maictext-zomaii",
  "icon": "logo.png",
  "license": "MIT",
  "pricing": "Free",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Notebooks",
    "Other"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zomaii/md-maic.git"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "markdown.md-maic.main",
        "category": "md-maic",
        "title": "%command-default%",
        "icon": {
          "light": "logo.png",
          "dark": "logo.png"
        },
        "when": "editorTextFocus && editorLangId == 'markdown'"
      },
      {
        "command": "markdown.md-maic.help",
        "category": "md-maic",
        "title": "%command-help%"
      },
      {
        "command": "markdown.md-maic.function.kbd",
        "category": "md-maic",
        "title": "%command-kbd%",
        "when": "editorTextFocus && editorLangId == 'markdown'"
      },
      {
        "command": "markdown.md-maic.function.than",
        "category": "md-maic",
        "title": "%command-element%",
        "when": "editorTextFocus && editorLangId == 'markdown'"
      },
      {
        "command": "markdown.md-maic.function.br",
        "category": "md-maic",
        "title": "%command-br%",
        "when":"editorTextFocus && editorLangId == 'markdown'"
      }
    ],
    "keybindings": [
      {
        "command": "markdown.md-maic.main",
        "key": "ctrl+shift+a",
        "when": "editorTextFocus && editorLangId == 'markdown'"
      },
      {
        "command": "markdown.md-maic.function.kbd",
        "key": "ctrl+alt+x",
        "when": "editorTextFocus && editorLangId == 'markdown'"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Markdown md-maic config [Maic Hub Club]",

    "properties": {
      "md-maic.RegisterManager": {
        "type": "object",
        "markdownDescription": "%conf-ReisterManager%",
        "default": {
          "appStamp": "Release 0.0.4",
          "conf.syntax": "syntax.json",
          "conf.cloud": "cloud.repo.json",
          "function.enabled":"kbd,than,br"
        },
        "additionalProperties": {
          "type": "string"
        }
      }
    }

    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.9.1",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "markdown-it": "^14.1.0",
    "node-fetch": "^3.3.2",
    "vscode-nls": "^5.2.0"
  }
}

{
  "name": "md-maic",
  "displayName": "md-maic",
  "description": "markdown dialect format",
  "version": "0.0.4",
  "publisher": "maictext-zomaii",
  "icon": "logo.png",
  "license": "MIT",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Notebooks",
    "Other"
  ],
  "activationEvents": [
    "onCommand:markdown.md-maic.processText",
    "onCommand:markdown.md-maic.function.kbd",
    "onCommand:markdown.md-maic.function.tag",
    "onLanguage:markdown"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zomaii/md-maic.git"
  },
  "main": "./out/extension.js",
  "contributes": {
    "localizations": [
      {
        "languageId": "zh-cn",
        "languageName": "Chinese",
        "localizedLanguageName": "中文",
        "translations": [
          {
            "id": "maictext-zomaii.md-maic",
            "path": "./lang.conf/zh-cn.json"
          }
        ]
      },
      {
        "languageId": "en",
        "languageName": "en",
        "translations": [
          {
            "id": "maictext-zomaii.md-maic",
            "path": "./lang.conf/en.json"
          }
        ]
      },
      {
        "languageId": "ja",
        "languageName": "日本語",
        "translations": [
          {
            "id": "maictext-zomaii.md-maic",
            "path": "./lang.conf/ja.json"
          }
        ]
      }
    ],
    "commands": [
      {
        "command": "markdown.md-maic.processText",
        "title": "md-maic"
      },
      {
        "command": "markdown.md-maic.function.kbd",
        "title": "md-maic.kbd"
      },
      {
        "command": "markdown.md-maic.function.tag",
        "title": "md-maic.tag"
      },
      {
        "command": "markdown.md-maic.root",
        "title": "md-maic.root"
      }
    ],
    "keybindings": [
      {
        "command": "markdown.md-maic.processText",
        "key": "ctrl+shift+a",
        "when": "editorTextFocus && editorLangId == 'markdown'"
      },
      {
        "command": "markdown.md-maic.function.kbd",
        "key": "ctrl+alt+x",
        "when": "editorTextFocus && editorLangId == 'markdown'"
      },
      {
        "command": "markdown.md-maic.function.tag",
        "key": "ctrl+alt+a",
        "when": "editorTextFocus && editorLangId == 'markdown'"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Markdown md-maic config",
      "properties": {
        "md-maic.appliedRegexFile": {
          "type": "string",
          "default": "def-a.txt",
          "description": "%appliedRegexFile%"
        },
        "md-maic.functionLab": {
          "type": "object",
          "description": "%fonctionLab%",
          "default": { "appStamp":"20241104-github"  },
          "additionalProperties": {
            "type": "string"
          }
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.9.1",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "markdown-it": "^14.1.0",
    "node-fetch": "^3.3.2",
    "vscode-nls": "^5.2.0"
  }
}
